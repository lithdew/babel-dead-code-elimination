{
  "name": "babel-dce-kit",
  "version": "0.0.0",
  "description": "Dead code elimination toolkit for Babel",
  "keywords": [
    "babel",
    "dce",
    "kit",
    "dead",
    "code",
    "elimination"
  ],
  "repository": "pcattori/babel-dce-kit",
  "author": "pcattori",
  "license": "MIT",
